{
  "name": "immo-feed",
  "version": "1.0.0",
  "description": "A extensible app for scraping property listings",
  "keywords": [
    "immobilier",
    "real estate",
    "scraper",
    "api",
    "notification"
  ],
  "scripts": {
    "test": "npm run test-spec && npm run test-selectors",
    "test-selectors": "tsc && node ./build/test/run-test.js",
    "test-spec": "tsc && mocha ./build/test/helper ./build/test/source",
    "build": "tsc",
    "start": "tsc && node ./build/run.js",
    "serve": "tsc && node ./build/api/server.js",
    "migrate": "./node_modules/.bin/migrate-mongo"
  },
  "author": "Tamara Robichet",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "json2csv": "^4.1.5",
    "mongoose": "^5.1.3",
    "puppeteer": "^1.5.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "typescript": "^2.9.1"
  },
  "devDependencies": {
    "@types/bull": "^3.3.11",
    "@types/cheerio": "^0.22.7",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/glob": "^5.0.35",
    "@types/json2csv": "^4.1.0",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.0.18",
    "@types/nightmare": "^2.10.4",
    "@types/node": "^9.6.23",
    "@types/puppeteer": "^1.3.4",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "@types/sinon": "^5.0.1",
    "chalk": "^2.4.1",
    "migrate-mongo": "^3.0.3",
    "mocha": "^5.2.0",
    "sinon": "^6.1.3"
  }
}
